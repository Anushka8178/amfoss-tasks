{
    "manifest_version": 3,
    "name": "Real-time Weather",
    "version": "1.0",
    "action":{
        "default_popup": "index.html",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }

    },
    
    "host_permissions":[
        "https://*.openweathermap.org/*"
    ],
   
      "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "background":{
        "service-worker":"script.js"
    }
}

